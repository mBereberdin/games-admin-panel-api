{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/logs.txt",
          "rollOnFileSizeLimit": true,
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "AdminDbContext": "Host=localhost;Port=6544;Database=AdminPanel;Username=admin;Password=123"
  },
  "Urls": "http://localhost:5002/",
  "IsUseSwagger": true
}
